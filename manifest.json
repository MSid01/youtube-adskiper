{
    "manifest_version": 3,
    "name": "Youtube Ad Skip Extension",
    "version": "1.0",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["adSkipper.js"],
        "run_at": "document_end"
      }
    ]
  }